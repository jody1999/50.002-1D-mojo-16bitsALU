module compare_16bits (
   input alufn[6],
   input z,
   input v,
   input n,
   output out[16]  
  ) {
  
  always {
    out[15:0] = 0;

    case(alufn[2:1]) {     // in LSB
    2b01:out[0] = z;        //A = B
    2b10:out[0] = n ^ v;    // A < B
    2b11:out[0] = z | (n ^ v);   // A <= B 
      
    default:
      out[0] = 0;
    }
  }
}